{
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "plugins": ["import", "react", "react-hooks", "prettier"],
  "rules": {
    "strict": 0,
    "quotes": ["warn", "double"],
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  }
}
